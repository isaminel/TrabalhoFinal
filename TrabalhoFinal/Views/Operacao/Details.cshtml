@model TrabalhoFinal.Models.Operacao
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.NomeOperacao</h2>

<ul>
    <li>ID: @Model.Id</li>
    <li>Operação: @Model.NomeOperacao</li>
    <li>Valor: @Model.Valor</li>
    <li>Conta Destino: @Model.NrContaDestino</li>
</ul>